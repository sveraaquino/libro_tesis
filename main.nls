  \item [{$ T $}]\begingroup Puntos catalogados correctamente.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ F $}]\begingroup Puntos catalogados correctamente.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ TP $}]\begingroup Verdadero positivo.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ FP $}]\begingroup Falso positivo.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ P $}]\begingroup Total positivo.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ N $}]\begingroup Total negativo.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ FN $}]\begingroup Falso negativo.\nomeqref {5.0}
		\nompageref{74}
  \item [{$ P^{'} $}]\begingroup Total positivo catalogado.\nomeqref {5.0}
